{
	"manifest_version": 2,
	"name": "Metal Detector",
	"version": "0.2",
	"description": "A tracking extension to help administrators find lost or stolen chromebooks",
	"icons": {
		"16": "icon16.png",
		"24": "icon24.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"64": "icon64.png",
		"128": "icon128.png",
		"256": "icon256.png",
		"512": "icon512.png"
	},
	"offline_enabled": false,
	"permissions": [
		"geolocation",
		"identity",
		"identity.email",
		"background",
		"enterprise.deviceAttributes",
		"enterprise.networkingAttributes",
		"alarms",
		"storage"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"storage": {
		"managed_schema": "schema.json"
	}
}
